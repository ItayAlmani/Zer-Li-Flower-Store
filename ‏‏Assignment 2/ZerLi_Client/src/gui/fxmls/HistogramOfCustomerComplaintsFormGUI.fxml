                  <Font name="System Bold" size="10.0" />
               </tickLabelFont></NumberAxis>
        </yAxis>
      </BarChart>
      <Label fx:id="lblFrom" layoutX="53.0" layoutY="64.0" text="From" />
      <Label fx:id="lblStartDate" layoutX="122.0" layoutY="64.0" text="StartDate" />
      <Label fx:id="lblTo" layoutX="233.0" layoutY="64.0" text="To" />
      <Label fx:id="lblEndDate" layoutX="305.0" layoutY="64.0" text="EndDate" />
   </children>
</AnchorPane>
